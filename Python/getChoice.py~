def getChoice(x,w):
	out = ''
	startX = w/37.66
	bubleH = w/52.65
	bubleMargin = w/210.9
	columnMargin = w/35.15
	endX = startX + bubleH
	for i in range(1,8):
		 if x >= startX - 2 and x <= endX + 2:
		     out = 'A'
		     break
		 startX = endX + bubleMargin
		 endX = startX + bubleH
		 if x >= startX - 2 and x <= endX + 2:
		     out = 'B'
		     break
		 startX = endX + bubleMargin
		 endX = startX + bubleH
		 if x >= startX - 2 and x <= endX + 2:
		     out = 'C'
		     break
		 startX = endX + bubleMargin
		 endX = startX + bubleH
		 if x >= startX - 2 and x <= endX + 2:
		     out = 'D'
		     break
		 startX = endX + bubleMargin
		 endX = startX + bubleH
		 if x >= startX - 2  and x <= endX + 2:
		     out = 'E'
		     break
		 startX = endX + columnMargin
		 endX = startX + bubleH
	return out
